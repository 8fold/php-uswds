{
  "name": "vendor/project",
  "description": "",
  "type": "library",
  "license": "MIT",
  "authors": [],
  "require": {
    "php": "^8.0",
    "8fold/php-html-builder": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.2.0",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Eightfold\\Uswds\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eightfold\\Uswds\\Tests\\": "./tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true,
  "scripts": {
    "prod": "@production",
    "production": ["@style", "@stan", "@test"],
    "style": "./vendor/bin/phpcs --standard=phpcs.xml -p ./src",
    "stan": "./vendor/bin/phpstan analyze",
    "test": "./vendor/bin/phpunit"
  }
}
